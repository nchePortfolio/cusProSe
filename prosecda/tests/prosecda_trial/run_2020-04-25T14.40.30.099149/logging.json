{
    "root": {
        "handlers": [
            "console",
            "info_handler"
        ],
        "level": "NOTSET",
        "propagate": false
    },
    "handlers": {
        "summary_handler": {
            "delay": false,
            "filename": "prosecda_trial/run_2020-04-25T14.40.30.099149/summary.log",
            "encoding": "utf8",
            "class": "logging.FileHandler",
            "mode": "a",
            "formatter": "simple",
            "level": "INFO"
        },
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "detailed",
            "level": "DEBUG"
        },
        "info_handler": {
            "delay": false,
            "filename": "prosecda_trial/run_2020-04-25T14.40.30.099149/info.log",
            "encoding": "utf8",
            "class": "logging.FileHandler",
            "mode": "a",
            "formatter": "detailed",
            "level": "DEBUG"
        }
    },
    "loggers": {
        "prosecda.lib": {
            "handlers": [
                "console",
                "info_handler"
            ],
            "level": "DEBUG",
            "propagate": false
        },
        "prosecda.lib.report": {
            "handlers": [
                "console",
                "summary_handler",
                "info_handler"
            ],
            "level": "DEBUG",
            "propagate": false
        }
    },
    "formatters": {
        "simple": {
            "format": "%(message)s"
        },
        "detailed": {
            "datefmt": "%H:%M:%S",
            "format": "%(asctime)s %(name)s %(levelname)s:\t%(message)s"
        }
    },
    "disable_existing_loggers": false,
    "version": 1
}