{
    "formatters": {
        "simple": {
            "format": "%(message)s"
        },
        "detailed": {
            "datefmt": "%H:%M:%S",
            "format": "%(asctime)s %(name)s %(levelname)s:\t%(message)s"
        }
    },
    "disable_existing_loggers": false,
    "root": {
        "handlers": [
            "console",
            "info_handler"
        ],
        "propagate": false,
        "level": "NOTSET"
    },
    "handlers": {
        "console": {
            "level": "DEBUG",
            "formatter": "detailed",
            "class": "logging.StreamHandler"
        },
        "summary_handler": {
            "level": "INFO",
            "filename": "prosecda_trial/run_2020-04-28T13.28.38.475835/summary.log",
            "encoding": "utf8",
            "formatter": "simple",
            "mode": "a",
            "delay": false,
            "class": "logging.FileHandler"
        },
        "info_handler": {
            "level": "DEBUG",
            "filename": "prosecda_trial/run_2020-04-28T13.28.38.475835/info.log",
            "encoding": "utf8",
            "formatter": "detailed",
            "mode": "a",
            "delay": false,
            "class": "logging.FileHandler"
        }
    },
    "loggers": {
        "prosecda.lib.report": {
            "handlers": [
                "console",
                "summary_handler",
                "info_handler"
            ],
            "propagate": false,
            "level": "DEBUG"
        },
        "prosecda.lib": {
            "handlers": [
                "console",
                "info_handler"
            ],
            "propagate": false,
            "level": "DEBUG"
        }
    },
    "version": 1
}